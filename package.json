{
  "name": "event-management-system",
  "version": "1.0.0",
  "description": "A comprehensive event management system with registration, ticketing, and analytics",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "build": "npm run build:client",
    "build:client": "cd client && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "migrate": "node server/scripts/migrate.js",
    "migrate:create": "node server/scripts/create-migration.js",
    "db:reset": "node server/scripts/reset-db.js",
    "seed": "node server/scripts/seed.js"
  },
  "keywords": [
    "event-management",
    "registration",
    "ticketing",
    "analytics",
    "nodejs",
    "express",
    "postgresql",
    "react"
  ],
  "author": "Event Management Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "pdf-lib": "^1.17.1",
    "pg": "^8.16.3",
    "sanitize-html": "^2.13.0",
    "better-sqlite3": "^8.7.0",
    "redis": "^4.6.7",
    "qrcode": "^1.5.3",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "stripe": "^14.7.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "eslint": "^8.55.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.5.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
